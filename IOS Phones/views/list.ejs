    <div class="container-fluid ">
        <div class="row">
    <% const len=posts.length; %> 
    <% var no=posts.slice(len-4,len) %>
    
    <% no.forEach(function(post){ %>
    <div class="col-lg-6">
        <div class="shape">
       <div class="imagelist">
   
           <a href="posts/<%=post._id%>"> <img src="/images/<%= post.imageid %>"/> </a>
           </div>
        <div class="deslist">
         <p><%= post.title %></p>
         <p><%=post.priceid %></p>
        </div>
        </div>
        </div>
       
<% }); %>
